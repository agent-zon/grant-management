{
  "name": "mcp",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "type": "module",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.21.1",
    "@sap-cloud-sdk/connectivity": "^3",
    "@sap-cloud-sdk/http-client": "^3",
    "@sap-cloud-sdk/resilience": "^3",
    "@sap/cds": "^9",
    "@sap/xssec": "^4",
    "express": "^4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "jwt-decode": "^4.0.0",
    "method-override": "^3.0.0",
    "axios": "^1.12.2",
    "body-parser": "^2.2.0",
    "compression": "^1.8.1",
    "cross-env": "^10.0.0"
  },
  "devDependencies": {
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/cds-types": "^0.14.0",
    "@cap-js/sqlite": "^2",
    "@types/node": "^22.0.0",
    "tsx": "^4",
    "typescript": "^5",
    "@types/express": "^4",
    "@sap/cds-dk": "^9.3.2"
  },
  "scripts": {
    "start": "npx cds-tsx serve",
    "watch": "npx cds-tsx watch",
    "debug": "npx cds debug "
  },
  "private": true,
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  },
  "cds": {
    "requires": {
      "[production]": {
        "auth": "ias"
      },
      "authorization_service": {
        "kind": "odata",
        "model": "srv/external/authorization-service"
      },
      "grant_management": {
        "kind": "odata",
        "model": "srv/external/grant-management"
      }
    }
  }
}
